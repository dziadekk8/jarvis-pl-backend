{
  "name": "jarvis-pl-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon --signal SIGINT app.js",
    "start:local": "cross-env GOOGLE_REDIRECT_URI=http://localhost:8080/oauth2/callback node app.js",
    "restart": "powershell -NoLogo -NoProfile -Command \"Get-Process node -ErrorAction SilentlyContinue | Stop-Process -Force; npm run start\""
